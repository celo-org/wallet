{
  "name": "@wallet/provider-url-service",
  "version": "1.0.0",
  "description": "Serves 3rd party URLs with API keys and relevant signatures",
  "author": "Celo",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "scripts": {
    "serve": "firebase serve --only functions",
    "update-firebase-alfajores-config": "firebase use alfajores && ./firebase-env-config.sh -e alfajores",
    "update-firebase-mainnet-config": "firebase use mainnet && ./firebase-env-config.sh -e mainnet",
    "deploy:providerUrl:alfajores": "yarn update-firebase-alfajores-config && firebase deploy --only functions:composeCicoProviderUrl",
    "deploy:providerUrl:mainnet": "yarn update-firebase-mainnet-config && firebase deploy --only functions:composeCicoProviderUrl",
    "deploy:initData:testnet": "firebase use testnet && firebase deploy --only functions:queryForUserInitData",
    "clean": "tsc -b . --clean",
    "build": "tsc -b .",
    "lint": "tslint --project ."
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.5.0",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.6.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "engines": {
    "node": "10"
  }
}
